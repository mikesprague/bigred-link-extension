{
  "name": "BigRed.link Browser Extension",
  "description": "BigRed.link Browser Extension for creating short URLs from current page",
  "author": "Michael Sprague",
  "short_name": "BigRed.link",
  "icons": {
    "16": "/images/icon-16.png",
    "32": "/images/icon-32.png",
    "48": "/images/icon-48.png",
    "128": "/images/icon-128.png"
  },
  "version": "1.1.1",
  "manifest_version": 3,
  "permissions": ["tabs", "clipboardWrite", "unlimitedStorage"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "/images/icon-16.png",
      "32": "/images/icon-32.png",
      "48": "/images/icon-48.png",
      "128": "/images/icon-128.png"
    }
  }
}
